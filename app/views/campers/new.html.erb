<h1>Enroll New Camper!</h1>

<% if flash[:errors] %>
    <% flash[:errors].each do |err| %>
    <p><%= err %></p>
<% end %>
<% end %>

<%= form_for @camper do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :age %>
    <%= f.number_field :age %>
    <%= f.submit "Enroll!" %>
    <% end %>